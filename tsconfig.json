{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": [
      "node",
      "aria-query",
      "estree",
      "graceful-fs",
      "html-minifier-terser",
      "json-schema",
      "prettier",
      "prop-types",
      "semver"
    ]
  },
  "include": ["src", "src/assets"]
}